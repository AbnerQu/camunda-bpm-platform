    <resource-root path="camunda-engine-plugin-connect-@project.version@.jar" />
  </resources>

  <dependencies>

    <module name="javax.api" />

    <module name="org.camunda.bpm.camunda-engine" />

    <module name="org.camunda.connect.camunda-connect-core" export="true" />
    <module name="org.camunda.connect.camunda-connect-http-client" export="true" services="import" />
    <module name="org.camunda.connect.camunda-connect-soap-http-client" services="import" />

  </dependencies>
</module>
